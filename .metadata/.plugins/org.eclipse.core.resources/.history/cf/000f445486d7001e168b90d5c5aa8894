package day2;
class a{
	int i=10;
}

class b extends a{
	void disp() {
		System.out.println(i);
	}
}
public class this_ay {

	public this_ay() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		b ob=new b();
		ob.disp();
		
		

	}

}
